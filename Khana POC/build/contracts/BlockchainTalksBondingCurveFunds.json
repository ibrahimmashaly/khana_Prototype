  "devdoc": {
    "author": "David Truong <david@truong.vc>",
    "methods": {
      "emergencyStop()": {
        "details": "An emergency stop for funding and withdrawing from the contracts.",
        "return": "A bool indicating if the emergency stop was successful."
      },
      "getTokenAddress()": {
        "details": "Return the value for the token contract address",
        "return": "The address of the token contract address"
      },
      "renounceOwnership()": {
        "details": "Allows the current owner to relinquish control of the contract."
      },
      "resumeContract()": {
        "details": "Restore contract.",
        "return": "A bool indicating if the restoring was successful."
      },
      "sendEth(uint256,address)": {
        "details": "Send ETH from this funds contract to the relevant account",
        "params": {
          "_account": "The account address to send the ETH to.",
          "_amount": "The amount of ETH to send in wei."
        }
      },
      "setTokenContract(address)": {
        "details": "Change the token contract associated with this funds contract.",
        "params": {
          "_tokenAddress": "The address of the token contract associated with this funds contract"
        }
      },
      "transferOwnership(address)": {
        "details": "Allows the current owner to transfer control of the contract to a newOwner.",
        "params": {
          "_newOwner": "The address to transfer ownership to."
        }
      }
    },
    "title": "Proof of Concept contract for the Funds contract for BlockchainTalks community"
  },
  "userdoc": {
    "methods": {
      "renounceOwnership()": {
        "notice": "Renouncing to ownership will leave the contract without an owner. It will not be possible to call the functions with the `onlyOwner` modifier anymore."
      },
      "sendEth(uint256,address)": {
        "notice": "This function is protected by the modifiers, ensuring only the authorised tokenContract can call this function."
      }
    }
  }
